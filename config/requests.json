{
    "getTicketsFromRepairDesk": {
      "schema": {
        "method": "GET",
        "host": "api.repairdesk.co",
        "path": "/api/web/v1/tickets",
        "headers": {
          "Content-Type": "application/json"
        },
        "query": {
          "keyword": "<%= email %>",
          "apiKey": "<%= iparam.repairDeskApiKey %>"
        }
      },
      "options": {
        "retryDelay": 1000
      }
    }
  }
  